{

  "coc.preferences.formatOnSaveFiletypes": ["elixir"],
  "codeLens.enable": true,
  "diagnostic.virtualText": true,
  "diagnostic.errorSign": "◉",
  "suggest.completionItemKindLabels": {
    "module": " ",
    "function": "ﬦ",
    "variable": "ﮂ"
  },
  "list.interactiveDebounceTime": 300,
  "suggest.snippetIndicator": "\u26a1 ",
  "languageserver": {
    "elixirLS": {
      "command": "~/.elixir-ls/elixir-ls/release/language_server.sh",
      "filetypes": ["elixir", "eelixir"],
      "settings": {
        "elixirLS": {
          "dialyzerEnabled": false
        }
      }
    }
  }
}
